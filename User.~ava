 *
 * Description
 *
 * @version 1.0 from 5/14/2020
 * @author 
 */

public class User {
  
  // start attributes
  private String Ip;
  private int Port;
  private Account myaccount;
  // end attributes
  
  public User(String Ip, int Port) {
    this.Ip = Ip;
    this.Port = Port;
    this.myaccount = null;
  }

  // start methods
  public String getIp() {
    return Ip;
  }

  public void setIp(String IpNew) {
    Ip = IpNew;
  }

  public int getPort() {
    return Port;
  }

  public void setPort(int PortNew) {
    Port = PortNew;
  }

  public Account getMyaccount() {
    return myaccount;
  }

  public void setMyaccount(Account myaccountNew) {
    myaccount = myaccountNew;
  }

  // end methods
} // end of User

